{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "arrow-parens": [2, "as-needed"]
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb/base",
    "eslint:recommended"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}
